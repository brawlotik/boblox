local teleportTargetObject = workspace:FindFirstChild("Rob_Alarm")
if teleportTargetObject then
    teleportTargetObject = teleportTargetObject:FindFirstChild("Robberys")
end
if teleportTargetObject then
    teleportTargetObject = teleportTargetObject:FindFirstChild("Bank")
end
if teleportTargetObject then
    teleportTargetObject = teleportTargetObject:FindFirstChild("GeldTisch")
end
if teleportTargetObject then
    teleportTargetObject = teleportTargetObject:FindFirstChild("ItemHolder")
end

if teleportTargetObject then
    local children = teleportTargetObject:GetChildren()
    if #children > 0 then
        local lastChild = children[#children] -- Берем последний объект
        smoothTeleport(lastChild.Position)
    else
        warn("ItemHolder пуст, нечего телепортировать!")
    end
else
    warn("Не удалось найти ItemHolder для телепортации!")
end
