local teleportTargetObject = workspace:FindFirstChild("Rob_Alarm")
if teleportTargetObject then
    teleportTargetObject = teleportTargetObject:FindFirstChild("Robberys")
end
if teleportTargetObject then
    teleportTargetObject = teleportTargetObject:FindFirstChild("Bank")
end
if teleportTargetObject then
    local children = teleportTargetObject:GetChildren()
    if #children >= 20 then
        teleportTargetObject = children[20]:FindFirstChild("ItemHolder")
    else
        teleportTargetObject = nil
    end
end
if teleportTargetObject then
    teleportTargetObject = teleportTargetObject:FindFirstChild("200")
end

if teleportTargetObject then
    smoothTeleport(teleportTargetObject.Position)
else
    warn("Не удалось найти объект для телепортации!")
end
